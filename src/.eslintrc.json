{
	"extends": [
		"react-app",
		"react-app/jest"
	],
	"rules": {
		"no-console": "warn",
		"prefer-const": "warn",
		"jsx-a11y/anchor-has-content": "off",
		"import/order": ["warn", {
			"groups": ["builtin", "external", "internal", "parent", "sibling", "index", "object", "type"],
			"newlines-between": "always-and-inside-groups"
			}]
	}
}